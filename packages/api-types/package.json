{
  "name": "@qflow/api-types",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "default": "./dist/auth/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/auth/index.ts src/knowledge-base/index.ts --dts",
    "dev": "tsup src/index.ts src/auth/index.ts src/knowledge-base/index.ts --dts --watch"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
